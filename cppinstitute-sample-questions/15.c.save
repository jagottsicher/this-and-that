
#include <stdio.h>

int main(void) {
    //prepare a file pointer
    FILE *f;
    // a char and a long
    char s[] = "To be or not to be";
    long i;
    
    // open filepointer for writing
    f = fopen("f","w+b");
    
    // placing string in file
    fputs(s,f);
    fseek(f,-2,SEEK_END);
    i = ftell(f);
    fclose(f);
    printf("%d",i);
return 0;
}
